    <script>
    $(document)
        .ready(function() {

            /**
             *
             * @author xujunxiong 20170324
             * @param  发布紧急公告
             *
             **/


            var carousel = function(number) {

                $('.admin-notify-content')
                    .css({ "top": "0px" });
                for (var i = 1; i < number; i++) {
                    (function(i) {
                        setTimeout(function timer() {
                            $('.admin-notify-content')
                                .animate({ "top": -20 * i + "px" });
                        }, i * 2500)
                    })(i);
                }
            };

            var announced = function(content) {

                var body = $("body");
                body.css("padding-top", "20px");
                $(".admin-notify")
                    .fadeIn(50);
                $(".admin-notify-content")
                    .html(content);
                var content_height = $(".admin-notify-content")
                    .height();
                var number = Math.floor(content_height / 20);

                carousel(number);
                if (number > 1) {
                    //setInterval IE目前不支持三参数
                    timer = setInterval(carousel, (number) * 2500, number);
                }

            };

            var ajaxFun = function() {
                var data = { key: 'urgentMsg' };
                var timer = null;
                jQuery.ajax({
                    url: '/ecTool/globalVar',
                    type: 'GET',
                    dataType: 'json',
                    data: data,
                    async: false,
                    success: function(res) {
                        if (!res.code) {
                            if (res.data.value) {
                                announced(res.data.value);
                            } else {
                                clearInterval(timer);
                            }
                        } else {
                            alert("获取紧急公告失败");
                        }
                    }
                });
            };

            ajaxFun();


            // 初始化语言选择器
            var cName = 'yunpian.lang=';
            var setLang = function(lang, actNow) {
                var langBtns = $('.lang-picker');
                langBtns.find('.cur')
                    .removeClass('cur');
                var langCookie = lang;

                switch (lang) {
                    case 'zh_CN':
                        actNow && langBtns.find('.lang-zh_CN')
                            .addClass('cur');
                        actNow && $('.leftnav')
                            .removeClass('en-nav');
                        actNow && $('.mainContainer')
                            .removeClass('en-nav');
                        break;
                    case 'en':
                        actNow && langBtns.find('.lang-en')
                            .addClass('cur');
                        actNow && $('.leftnav')
                            .addClass('en-nav');
                        actNow && $('.mainContainer')
                            .addClass('en-nav');
                        break;
                    case 'zh_TW':
                        actNow && langBtns.find('.lang-zh_TW')
                            .addClass('cur');
                        break;
                    default:
                        actNow && langBtns.find('.lang-zh_CN')
                            .addClass('cur');
                        actNow && $('.leftnav')
                            .addClass('en-nav');
                        actNow && $('.mainContainer')
                            .addClass('en-nav');
                        langCookie = 'zh_CN';
                }

                document.cookie = cName + langCookie + ';path=/';
            };
            var cookies = document.cookie.split(';');
            for (var i = 0; i < cookies.length; ++i) {
                var c = cookies[i].trim();
                if (c.indexOf(cName) >= 0) {
                    setLang(c.slice(cName.length, c.length), true);
                    break;
                }
            }
            $('.lang-picker > div')
                .click(function(e) {
                    var target = e.target;
                    target = target.className.length ? target : target.parentNode;

                    var lang = target.className.match(/lang-(\w+)/)[1];
                    setLang(lang);
                    location.reload();
                });
        });
    </script>
